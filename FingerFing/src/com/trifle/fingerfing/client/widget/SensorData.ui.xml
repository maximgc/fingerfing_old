				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Попытки:" />
					</g:customCell>
					<g:customCell>
						<p1:ProgressBarNew ui:field="pgFullCount" />
					</g:customCell>
					<g:customCell>
						<g:Label text="" ui:field="fullCount" />
					</g:customCell>
				</g:row> 
				<g:row>
					<g:customCell>
						<g:Label text="Правильно:" />
					</g:customCell>
					<g:customCell>
						<p1:ProgressBarNew ui:field="pgSuccessDensity" range="10" />
					</g:customCell>
					<g:customCell>
						<g:Label text="" ui:field="fullSuccessDensity" />
					</g:customCell>
				</g:row>
