				<g:row>
					<g:customCell>
						<g:Label text="Скорость:" />
					</g:customCell>
					<g:customCell>
						<p1:ProgressBarNew ui:field="pgMeanSpeed"  range="2000"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="" ui:field="lastMeanSpeed" />
					</g:customCell>
