<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Label text=" AT" ui:field="allTime"/>
			<g:Image width="20px" height="1px"/>
			<g:Label text=" MS" ui:field="meanSpeed"/>
			<g:Image width="20px" height="1px"/>
			<g:Label text=" ED" ui:field="errorDensity"/>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 