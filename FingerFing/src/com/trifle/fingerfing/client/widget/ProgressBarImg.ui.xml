<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HorizontalPanel width="200px">
		<g:Cell width="50%" >
			<g:Image url="fingerfing/progress-bar-bg.png" ui:field="imgPos" width="100%" height="10px" />
		</g:Cell>
		<g:Cell width="50%" height="">
			<g:Image url="fingerfing/progress-bar-bg-max.png" ui:field="imgMaxPos" width="100%" height="10px" />
		</g:Cell>
		<g:Cell width="">
			<g:Image ui:field="imgBlank" width="100%" height="10px"/>
		</g:Cell>
	</g:HorizontalPanel>
</ui:UiBinder> 