<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style
		type="com.trifle.fingerfing.client.widget.newage.ProgressBarBase.Styles">
		.important {
			font-weight: bold;
		}
		
		.progressbarInner {
			border: 1px solid #ccc;
			margin: 1px;
		}
		
		.progressbarBar {
			margin: 1px;
		}
		
		.progressbarFullbar {
			margin: 1px;
			background: #77f;
		}
		
		.progressbarBlankbar {
			margin: 1px;
			background: #eee;
		}
		</ui:style>
	<g:VerticalPanel>
		<g:Label ui:field="topLabel" horizontalAlignment="ALIGN_CENTER" />
		<g:Grid ui:field="elementBar" />
		<g:Label ui:field="valueLabel" horizontalAlignment="ALIGN_CENTER" />
	</g:VerticalPanel>
</ui:UiBinder>  